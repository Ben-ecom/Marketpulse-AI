# Server configuratie
PORT=3000
NODE_ENV=development

# Supabase configuratie
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# JWT configuratie
JWT_SECRET=your-jwt-secret
JWT_EXPIRES_IN=1h

# Logging configuratie
LOG_LEVEL=debug

# Scraping configuratie
USE_PUPPETEER=true
USE_APIFY=false
APIFY_API_TOKEN=your-apify-api-token

# Proxy configuratie
USE_PROXIES=false
PROXY_LIST=http://username:password@host1:port1,http://username:password@host2:port2

# Web Scraping API configuratie
USE_SCRAPING_API=false
SCRAPING_API_KEY=your-scraping-api-key
SCRAPING_API_URL=https://api.scrapingapi.com/scrape
SCRAPING_API_MONTHLY_LIMIT=23000
SCRAPING_API_PRIORITY=high # Prioriteit: 'high' (Scraping API eerst), 'medium' (Puppeteer eerst, dan API), 'low' (alleen als fallback)

# Amazon Product API configuratie (legacy)
USE_REAL_AMAZON_API=false
AMAZON_ACCESS_KEY=your-amazon-access-key
AMAZON_SECRET_KEY=your-amazon-secret-key
AMAZON_PARTNER_TAG=your-amazon-partner-tag

# Reddit API configuratie (legacy)
USE_REAL_REDDIT_API=false
REDDIT_CLIENT_ID=your-reddit-client-id
REDDIT_CLIENT_SECRET=your-reddit-client-secret
REDDIT_USERNAME=your-reddit-username
REDDIT_PASSWORD=your-reddit-password
